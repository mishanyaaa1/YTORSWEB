{
  "name": "ytorsweb-hosting",
  "version": "1.0.0",
  "description": "YTORSWEB - сайт для продажи техники",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "install:all": "npm install && cd server && npm install",
    "migrate": "cd server && npm run migrate",
    "seed": "cd server && npm run seed",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "logs": "pm2 logs",
    "monitor": "pm2 monit",
    "bot:setup": "cd server && node -e \"require('./index.js')\"",
    "bot:test": "node test-bot.js",
    "bot:chat-id": "node get-chat-id.js"
  },
  "keywords": ["ecommerce", "vehicles", "react", "nodejs", "express"],
  "author": "YTORSWEB Team",
  "license": "ISC",
  "dependencies": {
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.0",
    "styled-components": "^6.1.19",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "sqlite3": "^5.1.7",
    "helmet": "^7.1.0",
    "cookie-parser": "^1.4.7",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.4.0",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "sharp": "^0.34.3",
    "vite": "^7.1.0",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
